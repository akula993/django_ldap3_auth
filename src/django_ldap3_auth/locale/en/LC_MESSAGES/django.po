msgid "Например: dc.example.com"
msgstr "Example: dc.example.com"

msgid "Например: DC=example,DC=com"
msgstr "Example: DC=example,DC=com"

msgid "Например: ldapuser@example.com"
msgstr "Example: ldapuser@example.com"

msgid "Хост контроллера домена"
msgstr "Domain controller host"

msgid "Порт"
msgstr "Port"

msgid "Использовать SSL (LDAPS)"
msgstr "Use SSL (LDAPS)"

msgid "Base DN"
msgstr "Base DN"

msgid "Bind DN (сервисная учётка)"
msgstr "Bind DN (service account)"

msgid "Пароль сервисной учётки"
msgstr "Service account password"

msgid "Фильтр поиска пользователя"
msgstr "User search filter"

msgid "Атрибуты для выборки"
msgstr "Attributes to fetch"

msgid "Домен email по умолчанию"
msgstr "Default email domain"

msgid "Атрибут имени (first_name)"
msgstr "First name attribute"

msgid "Атрибут фамилии (last_name)"
msgstr "Last name attribute"

msgid "DN группы суперпользователей"
msgstr "Superuser group DN"

msgid "DN группы персонала (staff)"
msgstr "Staff group DN"

msgid "FQDN или IP контроллера домена (например, dc.example.com)."
msgstr "FQDN or IP of the domain controller (e.g. dc.example.com)."

msgid "389 для LDAP, 636 для LDAPS (SSL)."
msgstr "389 for LDAP, 636 for LDAPS (SSL)."

msgid "Если включено, будет использоваться LDAPS на порту 636."
msgstr "If enabled, LDAPS will be used on port 636."

msgid "Корневой DN домена (например, DC=example,DC=com). Будет предложен автоматически на основе хоста."
msgstr "Root DN of the domain (e.g. DC=example,DC=com). Will be suggested automatically based on the host."

msgid "UPN или DN сервисной учётной записи для поиска пользователей (например, ldapuser@example.com)."
msgstr "UPN or DN of the service account used to search for users (e.g. ldapuser@example.com)."

msgid "Рекомендуется задавать через переменные окружения. Ввод скрыт в админке."
msgstr "Recommended to use environment variables. Input is hidden in admin."

#, python-brace-format
msgid "Используется для поиска DN пользователя. {username} будет подставлен автоматически."
msgstr "Used to search for user DN. {username} will be substituted automatically."

msgid "Список атрибутов AD, которые нужно получать вместе с DN."
msgstr "List of AD attributes to fetch together with DN."

msgid "Если у пользователя нет mail в AD — будет сгенерирован username@этот.домен."
msgstr "If user has no mail in AD, username@domain will be generated."

msgid "Имя поля AD, из которого брать имя пользователя (например, givenName)."
msgstr "AD field name used for user's first name (e.g. givenName)."

msgid "Имя поля AD, из которого брать фамилию пользователя (например, sn)."
msgstr "AD field name used for user's last name (e.g. sn)."

msgid "Члены этой группы получат права суперпользователя Django."
msgstr "Members of this group will have Django superuser privileges."

msgid "Члены этой группы получат доступ к админке (is_staff)."
msgstr "Members of this group will have admin access (is_staff)."

msgid "LDAP конфигурация"
msgstr "LDAP Configuration"

msgid "LDAP конфигурация (одна запись)"
msgstr "LDAP Configuration (single record)"
